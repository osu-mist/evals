<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" >

    <class name="edu.osu.cws.evals.models.NolijCopy" table="nolij_copies">
        <id name="id" type="int" column="ID">
            <generator class="native">
                <param name="sequence">pass_seq</param>
            </generator>
        </id>

        <property name="appraisalId" type="int" column="APPRAISAL_ID"/>
        <property name="submitDate" type="timestamp" column="SUBMIT_DATE"/>
        <property name="filename" column="FILE_NAME"/>

    </class>

</hibernate-mapping>

